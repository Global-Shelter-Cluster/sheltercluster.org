function lcc_log_entity_metadata_wrapper_error($exception, $context) {
  watchdog(
    $context,
    'See '  . __FUNCTION__ . '() <pre>' . $exception->getTraceAsString() . '</pre>',
     NULL, WATCHDOG_ERROR
  );
}